<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="inputText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/poppins_regular</item>
        <item name="android:textColor">@color/text_color</item>
    </style>

    <style name="nama_wisata">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/main_text_color</item>
        <item name="android:fontFamily">@font/poppins_semi_bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="alamat_wisata_hasil_pencarian">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/main_text_color</item>
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="hasil_pencarian">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/main_text_color</item>
        <item name="android:fontFamily">@font/poppins_medium</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="hasil_pencarian_2">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/hijau</item>
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="hasil_pencarian_desc">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:fontFamily">@font/poppins_light</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="harga_tiket">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/hijau</item>
        <item name="android:fontFamily">@font/poppins_bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="harga_tiket_2">
        <item name="android:textSize">12sp</item>
